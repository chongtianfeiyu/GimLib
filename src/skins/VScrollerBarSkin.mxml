<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
			 xmlns:s="library://ns.adobe.com/flex/spark"
			 xmlns:fb="http://ns.adobe.com/flashbuilder/2009" minHeight="40">
	<fx:Metadata>
		<![CDATA[ 
		[HostComponent("spark.components.VScrollBar")]
		]]>
	</fx:Metadata> 
	<s:states>
		<s:State name="normal" />
		<s:State name="disabled" />
		<s:State name="inactive" />
	</s:states>
	<s:Group top="0" horizontalCenter="0" bottom="0" left="0" right="0">
		<s:Button id="track" top="0" bottom="0" width="2" focusEnabled="false"
				  skinClass="spark.skins.spark.VSliderTrackSkin"/>
		<s:Button id="thumb" width="8" maxHeight="6" buttonMode="true" focusEnabled="false" horizontalCenter="0"
				  skinClass="spark.skins.spark.HSliderThumbSkin"/>
	</s:Group>
</s:SparkSkin>
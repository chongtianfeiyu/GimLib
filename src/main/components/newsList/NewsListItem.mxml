<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009"
		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:mx="library://ns.adobe.com/flex/mx">
	<fx:Script>
		<![CDATA[
			[Bindable] public var text:String;
			public var url:String;
		]]>
	</fx:Script>
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	<s:Button width="100%" height="100%" label="" color="#353535" cornerRadius="4" fontSize="18"
			  fontWeight="bold" textJustify="distribute" toolTip=""/>
	<s:Label width="100%" height="100%" color="#353535" fontSize="18" fontWeight="bold"
			 lineBreak="explicit" maxDisplayedLines="1" mouseEnabled="false" paddingLeft="10"
			 paddingRight="10" text="{text}" textAlign="justify" verticalAlign="middle"/>
</s:Group>
